<?xml version="1.0" encoding="UTF-8"?>
<plugin
  name="Eclipse Core Runtime"
  id="org.eclipse.core.runtime"
  version="1.0"
  provider-name="IBM">

  <runtime>
    <library name="runtime.jar">
      <export name="*"/>
    </library>
  </runtime>

  <requires>
    <import plugin="org.apache.xerces"/>
  </requires>

  <extension-point name="Applications" id="applications"/>
  <extension-point name="URL Stream Handlers" id="urlHandlers"/>

</plugin>
