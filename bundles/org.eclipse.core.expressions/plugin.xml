<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.eclipse.core.expressions"
   name="%pluginName"
   version="3.0.0"
   provider-name="%providerName"
   class="org.eclipse.core.internal.expressions.ExpressionPlugin">

   <runtime>
      <library name="expressions.jar">
         <export name="*"/>
         <packages prefixes="org.eclipse.core.expressions,org.eclipse.core.internal.expressions"/>
      </library>
   </runtime>

   <requires>
   	  <import plugin="org.eclipse.core.runtime"/>
   	  <import plugin="org.eclipse.core.runtime.compatibility" optional="true"/>
   </requires>

   <extension-point id="propertyTesters" name="%propertyTesterExtensionPoint" schema="schema/propertyTesters.exsd"/>

</plugin>
